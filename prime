package com.media;

public class prime {

	public static void main(String[] args) {
		int i,m=0,c=0;
		int n=3;
		m=n/2;
		if(n==0||n==1)
		{
			System.out.println(n+"Not prime");
		}
		else
		{
			for (i=2;i<=m;i++)
			{
				if(n%i==0) 
				{
					System.out.println(n+"Not a prime");
					c=1;
					break;
				}
			}
			if (c==0)
			{
				System.out.println(n+"is a prime number");
			}
		}

	}

}
